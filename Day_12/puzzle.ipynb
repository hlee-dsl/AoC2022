   "source": [
    "from queue import PriorityQueue\n",
    "q = PriorityQueue()\n",
    "\n",
    "q.put((2, \"Smith\"))\n",
    "q.put((1, \"Jones\"))\n",
    "q.put((4, \"Wilson\"))\n",
    "\n",
    "next = q.get()\n",
    "print(next)\n",
    "\n",
    "print(q.empty())\n",
    "print(q.full())\n",
    "\n",
    "q.put((3, \"Collins\"))\n",
    "print(q.get())\n",
    "\n",
    "while not q.empty():\n",
    "     print(q.get())\n",
    "\n",
    "print(q.empty())"
   ]
  }
